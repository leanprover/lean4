private theorem ex₁ : ∀ (a : Nat), 2 ∣ a → 2 ∣ a + 1 → False :=
fun a h₁ h₂ => ex₁._proof_1_1 a h₁ h₂
private theorem ex₂ : ∀ (a b : Nat), 2 ∣ a + 1 → 2 ∣ b + a → 2 ∣ b + 2 * a → False :=
fun a b h₀ h₁ h₂ => ex₂._proof_1_1 a b h₀ h₁ h₂
private theorem ex₃ : ∀ (a b : Nat),
  2 ∣ a + 1 → 3 ∣ a + 3 * b + a → 2 ∣ 2 * b + a + 3 → 3 ∣ 3 * b + 2 * a + 1 → False :=
fun a b x h₁ h₂ h₃ => ex₃._proof_1_1 a b h₁ h₂ h₃
private theorem ex₄ : ∀ (a b : Nat), 1 - 1 + 1 * 2 ∣ a + 1 → 2 ∣ b + a → 2 ∣ b + 2 * a → False :=
fun a b h₀ h₁ h₂ => ex₄._proof_1_1 a b h₀ h₁ h₂
private theorem ex₅ : ∀ (x y : Nat), 1 ≤ x + y → 100 ∣ x + y → 100 ≤ x + y :=
fun x y => ex₅._proof_1_1 x y

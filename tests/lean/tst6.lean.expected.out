                        b
                        (@trans N a b c (@and_eliml (@eq N a b) (@eq N b c) H1) (@and_elimr (@eq N a b) (@eq N b c) H1))
                        (@refl N b))
             (λ H1 : @eq N a d ∧ @eq N d c,
                @congrH a
                        b
                        c
                        b
                        (@trans N a d c (@and_eliml (@eq N a d) (@eq N d c) H1) (@and_elimr (@eq N a d) (@eq N d c) H1))
